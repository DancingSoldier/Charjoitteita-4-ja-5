#include <iostream>
#include <string>

class Auto {
public:
    std::string rek;
    int h_nop;
    int nop;
    int matk;

    // Konstruktori
    Auto(std::string rek, int h_nop) {
        this->rek = rek;
        this->h_nop = h_nop;
        this->nop = 0;
        this->matk = 0;
    }
};

int main() {
    Auto auto1("ABC-123", 142);

    std::cout << auto1.rek << ", " << auto1.h_nop << " km/h "
        << auto1.nop << " " << auto1.matk << std::endl;

    return 0;
}
